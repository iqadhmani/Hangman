<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iqhangmanpok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_pkball"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_pkball_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.iqhangmanpok.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.iqhangmanpok.GameActivity"
            android:label="@string/title_activity_game">
        </activity>
        <activity
            android:name="com.example.iqhangmanpok.NewPlayerActivity"
            android:label="@string/title_activity_newplayer">
        </activity>
        <activity
            android:name="com.example.iqhangmanpok.LeaderboardActivity"
            android:label="@string/title_activity_leaderboard">
        </activity>
        <receiver android:name="com.example.iqhangmanpok.AlarmReceiver"/>
    </application>

</manifest>